{
  "name": "connect-store",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./build",
    "start": "node ./build/index",
    "start-ts": "ts-node ./src/index",
    "lint": "tslint -c ./src/tslint.json ./src/**/*.ts",
    "prebuild": "yarn run lint && yarn run clean",
    "prepublishOnly": "yarn build",
    "start:example": "yarn build && webpack-dev-server --config ./example/webpack.config.js"
  },
  "version": "2.0.6",
  "private": false,
  "license": "MIT",
  "author": "MiseryLee <miserylee@foxmail.com>",
  "description": "Easily manage stores of React views.",
  "repository": {
    "url": "https://github.com/miserylee/connect-store"
  },
  "typings": "./build/index.d.ts",
  "pre-commit": [
    "prepublishOnly"
  ],
  "devDependencies": {
    "@types/node": "11.12.0",
    "@types/react": "^16.8.10",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.10",
    "pre-commit": "1.2.2",
    "react-dom": "^16.4.0",
    "style-loader": "^0.19.1",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-clean-code": "0.2.9",
    "typescript": "3.3.4000",
    "webpack": "^3.10.0"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
